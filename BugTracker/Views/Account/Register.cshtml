@model BugTracker.Models.RegisterViewModel
@{
    ViewBag.Title = "BuggyTracker - Register";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>

    <!-- start: Meta -->
    <meta charset="utf-8">
    <title>BuggyTracker - Home</title>
    <meta name="description" content="Perfectum Dashboard Bootstrap Admin Template.">
    <meta name="author" content="Łukasz Holeczek">
    <!-- end: Meta -->
    <!-- start: Mobile Specific -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- end: Mobile Specific -->
    <!-- start: CSS -->
    <link id="bootstrap-style" href="~/perfectum_dashboard_1_0_5/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/perfectum_dashboard_1_0_5/css/bootstrap-responsive.min.css" rel="stylesheet">
    <link id="base-style" href="~/perfectum_dashboard_1_0_5/css/style.css" rel="stylesheet">
    <link id="base-style-responsive" href="~/perfectum_dashboard_1_0_5/css/style-responsive.css" rel="stylesheet">

    <!-- start: Favicon -->
    <link rel="shortcut icon" href="~/perfectum_dashboard_1_0_5/img/bug.png">
    <!-- end: Favicon -->




</head>
<body>

    <!-- start: Header -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
            
                <a class="brand" href="@Url.Action("Index", "Home")"> <img alt="Perfectum Dashboard" src="~/perfectum_dashboard_1_0_5/img/bug.png" width="40" /> <span class="hidden-phone">BuggyTracker</span></a>

                @*<div class="pull-right">@Html.Partial("_LoginPartial")</div>*@


            </div>
        </div>
    </div>
    <!-- start: Header -->

    <div class="container-fluid">
        <div class="row-fluid">

            <!-- start: Main Menu -->
            <div class="span2 main-menu-span">
                <div class="nav-collapse sidebar-nav">

                </div><!--/.well -->
            </div><!--/span-->
            <!-- end: Main Menu -->


            <div id="content" class="span10 container">
                <!-- start: Content -->
                <img src="~/perfectum_dashboard_1_0_5/img/tracking_banner.jpg" width="1400"/>
            <hr />
                <h2>@ViewBag.Title.</h2>

                @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                {
                    @Html.AntiForgeryToken()
                    <h4>Create a new account.</h4>
                    <hr />
                    @Html.ValidationSummary("", new { @class = "text-danger" })
                    <div class="form-group">
                        @Html.LabelFor(m => m.FirstName, new { @class = "col-md-2 control-label" })
                        <div class="col-md-10">
                            @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.LastName, new { @class = "col-md-2 control-label" })
                        <div class="col-md-10">
                            @Html.TextBoxFor(m => m.LastName, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.DisplayName, new { @class = "col-md-2 control-label" })
                        <div class="col-md-10">
                            @Html.TextBoxFor(m => m.DisplayName, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.Email, new { @class = "col-md-2 control-label" })
                        <div class="col-md-10">
                            @Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.Password, new { @class = "col-md-2 control-label" })
                        <div class="col-md-10">
                            @Html.PasswordFor(m => m.Password, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(m => m.ConfirmPassword, new { @class = "col-md-2 control-label" })
                        <div class="col-md-10">
                            @Html.PasswordFor(m => m.ConfirmPassword, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            <input type="submit" class="btn btn-success" value="Register" />
                        </div>
                    </div>
                    <br />
                    @Html.ActionLink("Return to Home", "Index", "Home", null, new { @class = "btn btn-primary" })
                }

            </div><!--/fluid-row-->

        </div>

    </div><!--/.fluid-container-->
    <!-- start: JavaScript-->
    <script src="~/perfectum_dashboard_1_0_5/js/jquery-1.9.1.min.js"></script>
    <script src="~/perfectum_dashboard_1_0_5/js/jquery-migrate-1.0.0.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery-ui-1.10.0.custom.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.ui.touch-punch.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/bootstrap.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.cookie.js"></script>

    <script src='~/perfectum_dashboard_1_0_5/js/fullcalendar.min.js'></script>

    <script src='~/perfectum_dashboard_1_0_5/js/jquery.dataTables.min.js'></script>
    <script src="~/perfectum_dashboard_1_0_5/js/excanvas.js"></script>
    <script src="~/perfectum_dashboard_1_0_5/js/jquery.flot.min.js"></script>
    <script src="~/perfectum_dashboard_1_0_5/js/jquery.flot.pie.min.js"></script>
    <script src="~/perfectum_dashboard_1_0_5/js/jquery.flot.stack.js"></script>
    <script src="~/perfectum_dashboard_1_0_5/js/jquery.flot.resize.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.chosen.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.uniform.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.cleditor.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.noty.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.elfinder.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.raty.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.iphone.toggle.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.uploadify-3.1.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.gritter.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.imagesloaded.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.masonry.min.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.knob.js"></script>

    <script src="~/perfectum_dashboard_1_0_5/js/jquery.sparkline.min.js"></script>
    <script src="~/perfectum_dashboard_1_0_5/js/custom.js"></script>

    <!-- end: JavaScript-->

</body>
</html>


<!---------------------------------------------------------------->



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
